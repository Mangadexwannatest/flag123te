--inint
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/4DailRyz/1yrsZHUB/main/kavo%20ui"))()
local Window = Library.CreateLib("TITLE")


local Tab = Window:NewTab("TabName")
local Section = Tab:NewSection("Section Name")

local QuestTable = {}
for i,v in pairs(game.Workspace.Quest:GetDescendants()) do
    if v.Name == "Quest" then
        table.insert(QuestTable,v.Value)
    end
end
Section:NewDropdown("Select Quest", nil, QuestTable, function(bool)
    getgenv().Quest = bool
end)

Section:NewToggle("Auto Quest",nil,function (bool)
    getgenv().AutoQuest = bool
end)

task.spawn(function ()
    while wait() do 
        pcall(function ()
            if getgenv().AutoQuest then
                for i,v in pairs(game.Workspace.Quest:GetDescendants()) do
                    if v.Name == "Quest" then
                        if v.Value == getgenv().Quest and getgenv().NoQuest then
                            fireproximityprompt(v.Parent)
                        end
                    end
                end
            end
        end)
    end
end)

task.spawn(function ()
    while wait() do
        pcall(function ()
            if getgenv().AutoQuest then
                for i,v in pairs(game.Players.LocalPlayer:GetChildren()) do 
                    if v.Name == "PlayerGui" and v:FindFirstChild("QuestUI") == nil then
                        getgenv().NoQuest = true
                    elseif v:FindFirstChild("QuestUI") ~= nil then
                        getgenv().NoQuest = false
                    end
                end
            end
        end)
    end
end)

task.spawn(function ()
    while wait() do 
        pcall(function ()
            if getgenv().AutoQuest then
                for i,v in pairs(game.Workspace.Quest:GetDescendants()) do
                    if v.Name == "Quest" then
                        if v.Value == getgenv().Quest then
                            if getgenv().NoQuest then
                                game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.Parent.Parent.CFrame
                            end
                        end
                    end
                end
            end
        end)
    end
end)

task.spawn(function ()
    while wait() do 
        pcall(function ()
            if getgenv().AutoQuest then
                for i,v in pairs(game.Workspace.Quest:GetDescendants()) do
                    if v.Name == "Quest" then
                        if v.Value == getgenv().Quest then
                            if not getgenv().NoQuest then
                                for _,mon in pairs(game.Workspace.Lives:GetChildren()) do
                                    if string.find(mon.Name,getgenv().Quest) then
                                        if mon.Humanoid.Health ~= 0 and not getgenv().NoQuest then
                                            repeat wait()
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = mon.HumanoidRootPart.CFrame * CFrame.new(0,0,4)
                                        until mon.Humanoid.Health == 0 or mon.Humanoid.Health < 0 or not getgenv().AutoQuest or getgenv().NoQuest
                                        end
                                    end
                                end
                            end
                        end
                    end
                end
            end
        end)
    end
end)
